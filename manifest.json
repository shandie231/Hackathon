{
    "manifest_version": 3,
    "name": "Quokkas are happiness",
    "description": "Quokkas unlimited",
    "permissions": ["activeTab"],
    "version": "1.0",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["main.js", "quokka.js"]
    }],
    "action":{
        "default_popup": "index.html"
    }
}
